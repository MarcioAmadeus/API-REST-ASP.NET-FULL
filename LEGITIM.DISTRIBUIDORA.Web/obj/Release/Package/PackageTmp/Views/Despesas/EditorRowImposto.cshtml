@model LEGITIM.DISTRIBUIDORA.Web.Models.Despesas.ImpostosViewModel

<tr class="editorRow">
    @using (Html.BeginCollectionItem("ListaImpostos"))
    {
        @Html.HiddenFor(model => model.Id)

        <td>
            @Html.EditorFor(model => model.Receitainicial, new { htmlAttributes = new { @class = "form-control", @placeholder = "Receita Inicial" } })
            @Html.ValidationMessageFor(model => model.Receitainicial, "", new { @class = "text-danger" })
        </td>
        <td>
            @Html.EditorFor(model => model.ReceitaLimite, new { htmlAttributes = new { @class = "form-control", @placeholder = "Receita Limite" } })
            @Html.ValidationMessageFor(model => model.ReceitaLimite, "", new { @class = "text-danger" })
        </td>
        <td>
            @Html.EditorFor(model => model.AnexoI, new { htmlAttributes = new { @class = "form-control", @placeholder = "%Comércio" } })
            @Html.ValidationMessageFor(model => model.AnexoI, "", new { @class = "text-danger" })
        </td>
        <td>
            @Html.EditorFor(model => model.AnexoII, new { htmlAttributes = new { @class = "form-control", @placeholder = "%Indústria" } })
            @Html.ValidationMessageFor(model => model.AnexoII, "", new { @class = "text-danger" })
        </td>

        <td>
            @Html.EditorFor(model => model.AnexoIII, new { htmlAttributes = new { @class = "form-control", @placeholder = "%Serviço" } })
            @Html.ValidationMessageFor(model => model.AnexoIII, "", new { @class = "text-danger" })
        </td>
        <td>
            @Html.EditorFor(model => model.AnexoIV, new { htmlAttributes = new { @class = "form-control", @placeholder = "%Serviços Especializados" } })
            @Html.ValidationMessageFor(model => model.AnexoIV, "", new { @class = "text-danger" })
        </td>
        <td>
            <a href="#" class=" btn btn-danger btn-xs btnModal deleteRow" title="Excluir">
                <span><i class="fa fa-trash-o"></i></span>
            </a>
        </td>
    }
</tr>

