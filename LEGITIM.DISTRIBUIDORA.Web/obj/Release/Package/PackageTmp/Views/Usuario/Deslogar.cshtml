@model LEGITIM.DISTRIBUIDORA.Web.Models.Usuarios.UsuarioViewModel
@{
    ViewBag.Title = "Deslogar";
}


<div class="table-responsive">
    @using (Html.BeginForm("Deslogar", "Usuario", FormMethod.Post))
    {
    @Html.AntiForgeryToken()

        <div class="form-group ">
            @Html.HiddenFor(model => model.Id)
            @Html.LabelFor(model => model.Login, htmlAttributes: new { @class = "control-label" })
            <br />
            @Html.TextBoxFor(model => model.Login, new { @class = "form-control formfield", disabled = "disabled" })
        </div>
        <div class="form-group ">
            @Html.LabelFor(model => model.Perfil, htmlAttributes: new { @class = "control-label" })
            <br />
            @Html.TextBoxFor(model => model.Perfil, new { @class = "form-control formfield", disabled = "disabled" })
        </div>

        <div class="form-group">
            <button class="btn btn-primary" id="btnAdicionar" type="submit">
                <i class="fa fa-sign-out"></i> Deslogar
            </button>
            <button type="button" onclick="goBack()" class="btn btn-default" data-dismiss="modal"> Voltar </button>
        </div>
    }
</div>

